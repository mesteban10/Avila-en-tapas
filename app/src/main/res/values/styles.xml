<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MainView">
        <item name="android:layout_height">@dimen/image_height</item>
        <item name="android:layout_marginStart">@dimen/margin_start</item>
        <item name="android:layout_marginTop">@dimen/margin_top</item>
        <item name="android:layout_marginEnd">@dimen/margin_end</item>
    </style>

    <style name="OfferText">
        <item name="android:layout_marginStart">@dimen/margin_start</item>
        <item name="android:layout_marginTop">@dimen/margin_top</item>
        <item name="android:paddingStart">@dimen/padding_left_offer</item>
        <item name="android:paddingEnd">@dimen/padding_right_offer</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="NameRestaurant">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size_big</item>
    </style>

    <style name="FoodType">
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">@dimen/padding_general</item>
    </style>

    <style name="FootView">
        <item name="android:layout_marginStart">@dimen/margin_start</item>
        <item name="android:layout_marginTop">@dimen/margin_top</item>
        <item name="android:layout_marginEnd">@dimen/margin_end</item>
    </style>

    <style name="LikeView">
        <item name="android:layout_marginEnd">@dimen/margin_end</item>
        <item name="android:padding">@dimen/padding_general</item>
    </style>

    <style name="NuevoView">
        <item name="android:padding">@dimen/padding_general</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="RoundedFigureReview">
        <item name="android:layout_marginStart">@dimen/margin_start</item>
        <item name="android:padding">@dimen/padding_review</item>
    </style>

    <style name="RoundedFigureDelivery">
        <item name="android:padding">@dimen/padding_review</item>
    </style>

    <style name="RoundedFigureTimeDelivery">
        <item name="android:padding">@dimen/padding_general</item>
        <item name="android:layout_marginBottom">@dimen/margin_bottom_time_del</item>
        <item name="android:layout_marginStart">@dimen/margin_start</item>
    </style>

    <style name="MarginsInfo">
        <item name="android:layout_marginTop">@dimen/margin_info</item>
        <item name="android:layout_marginBottom">@dimen/margin_info</item>
        <item name="android:layout_marginEnd">@dimen/margin_info</item>
        <item name="android:layout_marginStart">@dimen/margin_info</item>
    </style>

    <style name="TextInfoAlert">
        <item name="android:layout_marginStart">@dimen/margin_info</item>
    </style>

    <style name="VineyardImageHeight">
        <item name="android:layout_height">@dimen/vineyard_image_height</item>
    </style>

    <style name="TextAvisosPadding">
        <item name="android:padding">@dimen/padding_text_avisos</item>
    </style>

    <style name="CardViewAlerts">
        <item name="android:layout_margin">@dimen/card_margin</item>
        <item name="cardCornerRadius">@dimen/corner_radious</item>
    </style>

    <style name="MarginStartAndPaddingTopTitlesDetailsAlert">
        <item name="android:layout_marginStart">@dimen/margin_start_details_titles_alert</item>
        <item name="android:paddingTop">@dimen/padding_top_details_title_alert</item>
    </style>

    <style name="LineSeparatorMarginAndHeight">
        <item name="android:layout_margin">@dimen/margin_separator_details_alert</item>
        <item name="android:layout_height">@dimen/height_separator_details_alert</item>
    </style>
</resources>